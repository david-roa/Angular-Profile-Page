{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "profile-page": {
            "projectType": "application",
            "schematics": {},
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/profile-page",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": false,
                        "assets": [
                            "src/david.ico",
                            "src/assets",
                            "src/manifest.webmanifest",
                            "src/firebase-messaging-sw.js"
                        ],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                            "src/styles.css",
                            "src/assets/fonts/froala/style.css",
                            "./node_modules/froala-editor/css/froala_editor.pkgd.min.css",
                            "./node_modules/froala-editor/css/froala_style.min.css"
                        ],
                        "scripts": [
                            "./node_modules/froala-editor/js/froala_editor.pkgd.min.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                "type": "initial",
                                "maximumWarning": "2mb",
                                "maximumError": "5mb"
                            }],
                            "serviceWorker": true,
                            "ngswConfigPath": "ngsw-config.json"
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "profile-page:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "profile-page:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "profile-page:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "src/david.ico",
                            "src/assets",
                            "src/manifest.webmanifest",
                            "src/firebase-messaging-sw.js"
                        ],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                            "src/styles.css",
                            "src/assets/fonts/froala/style.css",
                            "./node_modules/froala-editor/css/froala_editor.pkgd.min.css",
                            "./node_modules/froala-editor/css/froala_style.min.css"
                        ],
                        "scripts": [
                            "./node_modules/froala-editor/js/froala_editor.pkgd.min.js"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "tsconfig.app.json",
                            "tsconfig.spec.json",
                            "e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "profile-page:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "profile-page:serve:production"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "profile-page"
}